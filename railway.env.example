# Railway Environment Variables
# Copia este archivo y renómbralo a .env para desarrollo local

# Base de datos (Railway proporcionará DATABASE_URL automáticamente)
# DATABASE_URL=postgresql://username:password@host:port/database

# Configuración de la aplicación
NODE_ENV=production
PORT=3000

# API Keys (configura estas en Railway Dashboard)
# GEMINI_API_KEY=tu_api_key_aqui
# LINISCO_API_KEY=tu_api_key_aqui
# SUPABASE_URL=tu_supabase_url_aqui
# SUPABASE_ANON_KEY=tu_supabase_anon_key_aqui

# Configuración de CORS para producción
CORS_ORIGIN=https://tu-dominio.railway.app
